{
    "name": "acaiteria/payment",
    "description": "API payment",
    "type": "project",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Iury Gomes de Oliveira",
            "email": "iurygdeoliveira@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    },
    "autoload": {
        "psr-4": {
            "ApiPayment\\Faker\\": "ApiPayment/Faker/",
            "ApiPayment\\Consumers\\": "ApiPayment/Consumers/",
            "ApiPayment\\Producers\\": "ApiPayment/Producers/",
