    "require": {
        "php": "^7.4",
        "ext-pdo": "^7.4",
        "coffeecode/datalayer": "1.1.7",
        "php-amqplib/php-amqplib": "2.11.3",
        "pagarme/pagarme-php": "v4.1.1",
        "opis/json-schema": "1.0.19",
        "opis/string": "1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^v1.9.1",
        "friendsofphp/php-cs-fixer": "^v2.16.4",
        "squizlabs/php_codesniffer": "3.5.6"
